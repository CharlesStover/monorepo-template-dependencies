{
  "name": "@monorepo-template/tsconfig",
  "version": "1.1.0",
  "author": "Charles Stover <monorepo-template-tsconfig@charlesstover.com>",
  "description": "TypeScript configurations for monorepos",
  "homepage": "https://github.com/monorepo-template/dependencies/tree/main/packages/tsconfig#readme",
  "license": "MIT",
  "main": "./configurations/index.json",
  "module": "./configurations/index.json",
  "bugs": {
    "email": "monorepo-template-tsconfig@charlesstover.com",
    "url": "https://github.com/monorepo-template/dependencies/issues"
  },
  "exports": {
    ".": {
      "default": "./tsconfig.json"
    },
    "./react-application": {
      "default": "./configurations/react-application.json"
    },
    "./react-application/cypress": {
      "default": "./configurations/react-application--cypress.json"
    },
    "./react-application/development": {
      "default": "./configurations/react-application--development.json"
    },
    "./react-application/eslint": {
      "default": "./configurations/react-application--eslint.json"
    },
    "./react-application/no-jsx-runtime": {
      "default": "./configurations/react-application--no-jsx-runtime.json"
    },
    "./react-module": {
      "default": "./configurations/react-module.json"
    },
    "./react-module/development": {
      "default": "./configurations/react-module--development.json"
    },
    "./react-module/eslint": {
      "default": "./configurations/react-module--eslint.json"
    },
    "./react-module/no-jsx-runtime": {
      "default": "./configurations/react-module--no-jsx-runtime.json"
    }
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/CharlesStover"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/monorepo-template/dependencies.git"
  },
  "scripts": {
    "eslint": "eslint '**/*'",
    "eslint:fix": "eslint '**/*' --fix"
  },
  "devDependencies": {
    "@types/node": "^18.6.3",
    "eslint": "^8.21.0"
  },
  "peerDependencies": {
    "@types/node": "*"
  }
}
