{
  "name": "@monorepo-template/eslint-config-typescript",
  "version": "1.1.1",
  "author": "Charles Stover <monorepo-template-eslint-config-typescript@charlesstover.com>",
  "description": "TypeScript configuration for ESLint",
  "homepage": "https://github.com/monorepo-template/dependencies/tree/main/packages/eslint-config-typescript#readme",
  "license": "MIT",
  "main": "./rules/index.json",
  "type": "commonjs",
  "bugs": {
    "email": "monorepo-template-eslint-config-typescript@charlesstover.com",
    "url": "https://github.com/monorepo-template/dependencies/issues"
  },
  "exports": {
    ".": {
      "default": "./rules/index.json"
    },
    "./cjs": {
      "default": "./rules/cjs.json"
    },
    "./eslint": {
      "default": "./rules/eslint.json"
    },
    "./js": {
      "default": "./rules/js.json"
    },
    "./json": {
      "default": "./rules/json.json"
    },
    "./ts": {
      "default": "./rules/ts.json"
    }
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/CharlesStover"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/monorepo-template/dependencies.git"
  },
  "scripts": {
    "eslint": "eslint '**/*'",
    "eslint:fix": "eslint '**/*' --fix"
  },
  "devDependencies": {
    "eslint": "^8.15.0"
  }
}
