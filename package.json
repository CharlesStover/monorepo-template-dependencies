{
  "name": "@monorepo-template/dependencies",
  "version": "1.0.0",
  "license": "MIT",
  "packageManager": "yarn@3.2.3",
  "private": true,
  "type": "module",
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/CharlesStover"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "eslint": "eslint '**/*' --ignore-pattern 'packages/**/*'",
    "eslint:fix": "eslint '**/*' --fix --ignore-pattern 'packages/**/*'",
    "prepack": "yarn workspaces foreach --parallel --topological-dev run prepack",
    "prepublish": "yarn eslint && yarn test && yarn workspaces foreach --parallel --topological-dev run prepublish",
    "test": "monorepo-template-test"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@monorepo-template/eslint-config": "workspace:^",
    "@monorepo-template/test": "workspace:^",
    "@monorepo-template/tsconfig": "workspace:^",
    "@types/node": "^18.7.22",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@yarnpkg/sdks": "^3.0.0-rc.21",
    "ansi-styles": "^6.1.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3"
  }
}
