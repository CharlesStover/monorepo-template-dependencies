{
  "name": "@charlesstover/monorepo-template-dependencies",
  "version": "1.0.0",
  "license": "MIT",
  "packageManager": "yarn@3.1.1",
  "private": true,
  "type": "module",
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/CharlesStover"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "coverage:eslint": "yarn workspace @monorepo-template/coverage run eslint",
    "cypress-coverage-config-override:eslint": "yarn workspace @monorepo-template/cypress-coverage-config-override run eslint",
    "cypress-coverage-config-override:jest": "yarn workspace @monorepo-template/cypress-coverage-config-override run jest",
    "eslint": "eslint '**/*' --ignore-pattern 'packages/**/*'",
    "eslint-config-typescript:eslint": "yarn workspace @monorepo-template/eslint-config-typescript run eslint",
    "eslint-config-typescript-fixable:eslint": "yarn workspace @monorepo-template/eslint-config-typescript-fixable run eslint",
    "merge-coverage:eslint": "yarn workspace @monorepo-template/merge-coverage run eslint",
    "test": "monorepo-template-test",
    "test:eslint": "yarn workspace @monorepo-template/test run eslint",
    "test:jest": "yarn workspace @monorepo-template/test run jest"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@monorepo-template/eslint-config-typescript": "workspace:^",
    "@monorepo-template/eslint-config-typescript-fixable": "workspace:^",
    "@monorepo-template/test": "workspace:^",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@yarnpkg/sdks": "^2.5.1-rc.6",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  }
}
